{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "CLAUDE_NO_TELEMETRY": "1",
    "EDITOR": "nvim"
  },
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash:*",
      "Read:*",
      "Write:*",
      "Edit:*",
      "MultiEdit:*",
      "Glob:*",
      "Grep:*",
      "LS:*",
      "TodoWrite:*",
      "WebFetch:*",
      "WebSearch:*",
      "Task:*",
      "ExitPlanMode:*",
      "NotebookRead:*",
      "NotebookEdit:*"
    ],
    "deny": [
      "Bash(sudo:*)",
      "Bash(* sudo *)",
      "Bash(chmod 777:*)",
      "Bash(curl * | bash)",
      "Bash(wget * | bash)"
    ]
  },
  "enableAllProjectMcpServers": false,
  "feedbackSurveyState": {
    "lastShownTime": 1754115106610
  }
}